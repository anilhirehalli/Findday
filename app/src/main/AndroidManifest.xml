<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="engin.findday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/engin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/engin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sunday" />
        <activity android:name=".monday" />
        <activity android:name=".tuesday" />
        <activity android:name=".wednesday" />
        <activity android:name=".friday" />
        <activity android:name=".thrusday" />
        <activity android:name=".saturday" />
        <activity android:name=".error"></activity>
    </application>

</manifest>